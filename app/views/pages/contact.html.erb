<!DOCTYPE html>
<html>
  <body>
    <h3>Alara Wholefoods</h3>
    <p>110-112 Camley St, London N1C 4PF</p>
    <div id="map"></div>
    <script>
      function initMap() {
        var uluru = {lat: 51.539285, lng: -0.130806};
        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 16,
          center: uluru
        });
        var marker = new google.maps.Marker({
          position: uluru,
          map: map
        });
      }
    </script>
    <script async defer
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCrqy4gYBR2vuLmiQc6Jlba_RVBeAbpt3E&callback=initMap">
    </script>
  </body>
</html>
